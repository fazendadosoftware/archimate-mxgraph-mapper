{
  "name": "archimate-mxgraph-mapper",
  "author": "LeanIX GmbH",
  "version": "0.4.1",
  "private": true,
  "main": "electron/electron.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fazendadosoftware/archimate-mxgraph-mapper.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
    "app:build": "npm run build:for:electron && npm run electron:builder",
    "test": "jest"
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "buffer": "^6.0.3",
    "comlink": "^4.3.1",
    "date-fns": "^2.25.0",
    "flexsearch": "^0.7.21",
    "jsoneditor": "^9.5.7",
    "jwt-decode": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "mxgraph": "^4.2.2",
    "stream": "^0.0.2",
    "sweetalert2": "^11.1.9",
    "v-wave": "^1.3.1",
    "vue": "^3.1.5",
    "workerize-loader": "^1.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.1",
    "@types/jest": "^27.0.3",
    "@types/jsoneditor": "^9.5.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.20",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^27.4.2",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.17",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.fazendadosoftware.archimateMxgraphMapper",
    "productName": "Archimate MXGraph Mapper",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
